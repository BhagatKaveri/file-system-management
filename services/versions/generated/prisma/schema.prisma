generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_VERSION")
}

model Version {
  id         String   @id @default(uuid())
  documentId String
  version    String
  fileUrl    String
  uploadedAt DateTime @default(now())
}
