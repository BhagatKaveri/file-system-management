// services/users/prisma/schema.prisma

datasource userDb {
  provider = "postgresql"
  url      = env("DATABASE_URL_USER")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
